@Scripts.Render("~/bundles/courses")
<script>
    $(document).ready(function () {
        $("#courses").addClass("page-scroll-active");
    });

    jQuery(".card__container").attrchange({
        trackValues: false,
        callback: function (event) {
            if (event.attributeName === "class") {
                if (jQuery(this).hasClass("card__container--closed")) {
                    jQuery("body").getNiceScroll().show();
                    jQuery(".card__container").getNiceScroll().hide();
                } else {
                    if (jQuery(window).height() < $(this).find(".card__image").height() + $(this).find(".card__content").height()) {
                        jQuery("body").getNiceScroll().hide();
                        jQuery(this).niceScroll({ zindex: "999", horizrailenabled: false });
                    } else {
                        jQuery(this).getNiceScroll().remove();
                        jQuery("body").getNiceScroll().hide();
                    }
                }
            }
        }
    });
    jQuery(".card__container").on("mouseover mousemove", function () {
        if (!jQuery(this).hasClass("card__container--closed")) {
            jQuery(this).getNiceScroll().resize();
            jQuery("body").getNiceScroll().hide();
        }
        else {
            jQuery(this).getNiceScroll().hide();
        }
    });

    jQuery(".card__container").bind("scroll", this, function () {
        if (!jQuery(this).hasClass("card__container--closed")) {
            jQuery("body").getNiceScroll().hide();
            jQuery(this).getNiceScroll().show();
        }
    });

    jQuery(".card__btn-close").bind("click", this, function () {
        jQuery(this).parent().getNiceScroll().hide();
        jQuery("html").getNiceScroll().show();
    });

    var englishCard = $("#cardEnglish");
    var spanishCard = $("#cardSpanish");
    var germanCard = $("#cardGerman");
    var frenchCard = $("#cardFrench");
    var italCard = $("#cardItal");
    var portuCard = $("#cardPortu");

    jQuery("main").addClass("body--hidden");

